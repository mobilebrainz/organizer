<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane onMouseClicked="#onEditPairBtn" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.khodko.organizer.controller.PairController">
    <GridPane prefHeight="80.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
              AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="20.0" prefWidth="40.0"/>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="100.0" prefWidth="250.0"/>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="50.0" prefWidth="50.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        </rowConstraints>
        <Label fx:id="lessonLabel" text="Label" GridPane.columnIndex="1"/>
        <Label fx:id="teacherLabel" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
        <Label fx:id="typeLabel" text="Label" GridPane.columnIndex="2"/>
        <Label fx:id="cabinetLabel" text="Label" GridPane.columnIndex="2" GridPane.rowIndex="1"/>
        <Label fx:id="numPairLabel" text="Label" GridPane.rowSpan="2">
            <GridPane.margin>
                <Insets left="10.0"/>
            </GridPane.margin>
        </Label>
    </GridPane>
</AnchorPane>
